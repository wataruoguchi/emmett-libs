{
  "name": "@wataruoguchi/emmett-crypto-shredding-kysely",
  "publishConfig": {
    "access": "public"
  },
  "version": "2.1.2",
  "description": "Kysely adapters for Emmett Crypto Shredding",
  "author": "Wataru Oguchi",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/wataruoguchi/emmett-libs.git"
  },
  "homepage": "https://github.com/wataruoguchi/emmett-libs",
  "bugs": "https://github.com/wataruoguchi/emmett-libs/issues",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "database"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "npm run resolve-deps && rm -rf dist && tsc -p tsconfig.build.json && tsup src/index.ts",
    "type-check": "tsc --noEmit",
    "tc": "npm run type-check",
    "pretest": "npm run resolve-deps",
    "preinstall": "cd ../emmett-crypto-shredding && npm install && npm run build",
    "resolve-deps": "cd ../emmett-crypto-shredding && npm install && npm run build",
    "test": "npm run type-check && vitest run",
    "test:coverage": "vitest run --coverage"
  },
  "devDependencies": {
    "@types/node": "^24.9.2",
    "@vitest/coverage-v8": "^3.2.4",
    "kysely": "^0.28.7",
    "tsup": "^8.5.0",
    "typescript": "^5.8.3",
    "vitest": "^3.2.4"
  },
  "peerDependencies": {
    "@event-driven-io/emmett": "0",
    "@wataruoguchi/emmett-crypto-shredding": "file:../emmett-crypto-shredding",
    "kysely": "0"
  },
  "optionalDependencies": {
    "@rollup/rollup-linux-x64-gnu": "4.9.5",
    "@wataruoguchi/emmett-crypto-shredding": "1"
  },
  "keywords": [
    "emmett",
    "crypto-shredding",
    "kysely",
    "postgres",
    "postgresql",
    "event-sourcing"
  ]
}
